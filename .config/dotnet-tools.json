{
  "version": 1,
  "isRoot": true,
  "tools": {
    "husky": {
      "version": "0.7.1",
      "commands": [
        "husky"
      ],
      "rollForward": false
    },
    "csharpier": {
      "version": "0.29.2",
      "commands": [
        "dotnet-csharpier"
      ],
      "args": [ "csharpier", "${staged}" ],
      "include": [ "**/*.cs","**/*.css","**/*.js","**/*.razor","**/*.sql" ],
      "rollForward": false
    }
  }
}
